<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" name="com.smartitengineering.user.domain.User" table="smart_user">
    <id column="id" name="id" type="java.lang.Integer">
      <generator class="increment">
            </generator>
    </id>
    <version column="version" name="version"/>
    <property column="name" length="255" name="name" type="java.lang.String" />
    <property column="object_id" length="255" name="objectID" type="java.lang.String" unique-key="UK_OBJECT_ORGANIZATION"/>
    <many-to-one cascade="none" column="organization_id" lazy="false" name="organization" unique-key="UK_OBJECT_ORGANIZATION"/>
    <many-to-one cascade="none" column="parent_object" lazy="false" name="parentObject"/>
    <property column="last_modified_date" name="lastModifiedDate" type="java.util.Date"/>
  </class>
</hibernate-mapping>
